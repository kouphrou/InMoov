;----------------------------- NEOPIXEL RX/TX CONFIG ----------------------------------------
[MAIN]
isNeopixelActivated=True
NeopixelMaster=left
;NeopixelMaster= right / left
NeopixelMasterPort=COM7
;NeopixelMasterPort= COMx for usb / Serialx for rx-tx ( Serial is case sensitive )
[NEOPIXEL]
pin=2
numberOfPixel=128

;Background neopixel reactions
[BASIC_REACTIONS]
boot_green=True
downloadSomething_blue=True
error_red=True
flash_when_speak = True
;----------------------------------- END --------------------------------------------------


